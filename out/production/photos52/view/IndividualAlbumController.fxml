<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="model.IndividualAlbumController" prefHeight="400.0" prefWidth="600.0">
    <GridPane fx:id="album_grid" prefWidth="190" prefHeight="200" visible="false">
        <rowConstraints>
            <RowConstraints prefHeight="100"/>
            <RowConstraints prefHeight="33"/>
            <RowConstraints prefHeight="34"/>
            <RowConstraints prefHeight="33"/>
        </rowConstraints>
        <ImageView fx:id ="image" fitWidth="100" fitHeight="100" GridPane.rowIndex="0"/>
        <Label text="Name of Album" alignment="CENTER" GridPane.rowIndex="1"/>
        <Label text="Number of photos in Album" alignment="CENTER" GridPane.rowIndex="2"/>
        <Label text="Dates of Album" alignment="CENTER" GridPane.rowIndex="3"/>
    </GridPane>
</AnchorPane>
